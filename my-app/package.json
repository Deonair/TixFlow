{
  "name": "tixflow-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:all": "concurrently -n back,front -c green,blue \"npm --prefix backend run dev\" \"vite\"",
    "dev:stripe": "stripe listen --forward-to http://localhost:5050/api/webhook/stripe",
    "dev:all:stripe": "concurrently -n back,front,stripe -c green,blue,magenta \"npm --prefix backend run dev\" \"vite\" \"stripe listen --forward-to http://localhost:5050/api/webhook/stripe\""
  },
  "dependencies": {
    "@stripe/stripe-js": "^8.5.3",
    "gsap": "^3.13.0",
    "preline": "^3.2.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.14",
    "cypress": "^15.6.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}
